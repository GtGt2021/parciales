#ifndef ARCADE_H_
#define ARCADE_H_
#define NACIONALIDAD_LEN 24
#define VIDEOJUEGO_LEN 63
typedef struct{
	int tipo_salon;
	int id_salon;
	int id_arcade;
	char nacionalidad_arcade [NACIONALIDAD_LEN];
	char nombreJuego_arcade [VIDEOJUEGO_LEN];
	int isEmpty_salon;
}eArcades;






#endif
